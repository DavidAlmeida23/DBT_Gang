void C8051Arch::decode()
{
	uint8_t op = FETCH;
	uint8_t subOp = op & 0x0F;
	uint8_t regN = op & 0x7;

//	helper_CC_lazyEv_SNIFFER_BACKDOOR();
//	target->gen_helper(&CTargetArchitecture::helper_call);

	uint8_t range1 = (op >> 1), range2 = (op & 0x1);

	switch  (range1) {
	case 0x0:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x0(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x1(subOp, regN);
	    break;
	  }
	break;
	case 0x1:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x2(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x3(subOp, regN);
	    break;
	  }
	break;
	case 0x2:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x4(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x5(subOp, regN);
	    break;
	  }
	break;
	case 0x3:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x6(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x7(subOp, regN);
	    break;
	  }
	break;
	case 0x4:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x8(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x9(subOp, regN);
	    break;
	  }
	break;
	case 0x5:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0xa(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0xb(subOp, regN);
	    break;
	  }
	break;
	case 0x6:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0xc(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0xd(subOp, regN);
	    break;
	  }
	break;
	case 0x7:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0xe(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0xf(subOp, regN);
	    break;
	  }
	break;
	case 0x8:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x10(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x11(subOp, regN);
	    break;
	  }
	break;
	case 0x9:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x12(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x13(subOp, regN);
	    break;
	  }
	break;
	case 0xa:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x14(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x15(subOp, regN);
	    break;
	  }
	break;
	case 0xb:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x16(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x17(subOp, regN);
	    break;
	  }
	break;
	case 0xc:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x18(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x19(subOp, regN);
	    break;
	  }
	break;
	case 0xd:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x1a(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x1b(subOp, regN);
	    break;
	  }
	break;
	case 0xe:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x1c(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x1d(subOp, regN);
	    break;
	  }
	break;
	case 0xf:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x1e(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x1f(subOp, regN);
	    break;
	  }
	break;
	case 0x10:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x20(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x21(subOp, regN);
	    break;
	  }
	break;
	case 0x11:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x22(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x23(subOp, regN);
	    break;
	  }
	break;
	case 0x12:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x24(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x25(subOp, regN);
	    break;
	  }
	break;
	case 0x13:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x26(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x27(subOp, regN);
	    break;
	  }
	break;
	case 0x14:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x28(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x29(subOp, regN);
	    break;
	  }
	break;
	case 0x15:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x2a(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x2b(subOp, regN);
	    break;
	  }
	break;
	case 0x16:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x2c(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x2d(subOp, regN);
