	case 0x1c:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x38(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x39(subOp, regN);
	    break;
	  }
	break;
	case 0x1d:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x3a(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x3b(subOp, regN);
	    break;
	  }
	break;
	case 0x1e:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x3c(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x3d(subOp, regN);
	    break;
	  }
	break;
	case 0x1f:
	  switch (range2) {
