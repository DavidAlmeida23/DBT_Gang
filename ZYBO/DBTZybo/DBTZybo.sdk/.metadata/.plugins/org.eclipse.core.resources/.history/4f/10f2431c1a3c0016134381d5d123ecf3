}

void  C8051Arch::fineDecode_0xcb(uint8_t subOp, uint8_t Rn){    // XCH A, Rn
	target->gen_ld8(tReg1, Rn); // Load Rn.
	target->gen_ld8(tReg2, A); // Load A.
