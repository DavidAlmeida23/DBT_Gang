	    break;
	  case 0x1:
	    fineDecode_0x3f(subOp, regN);
	    break;
	  }
	break;
	case 0x20:
	  switch (range2) {
	  case 0x0:
	    fineDecode_0x40(subOp, regN);
	    break;
	  case 0x1:
	    fineDecode_0x41(subOp, regN);
	    break;
	  }
	break;
	case 0x21:
	  switch (range2) {
